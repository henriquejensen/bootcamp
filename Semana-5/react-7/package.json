{
  "name": "test_files",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "codenation-jest-score": "https://github.com/thecodenation/codenation-jest-score/tarball/master",
    "react": "^16.9.0",
    "react-bulma-components": "^2.3.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "puppeteer": "^1.18.1",
    "jest-puppeteer": "^4.2.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "cross-env react-scripts build",
    "test": "cross-env react-scripts test --env=jsdom",
    "test:e2e": "jest --detectOpenHandles",
    "eject": "react-scripts eject",
    "exec": "jest --detectOpenHandles --json",
    "submit": "codenation-jest-score ./score-map.json",
    "preset": "jest-puppeteer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
